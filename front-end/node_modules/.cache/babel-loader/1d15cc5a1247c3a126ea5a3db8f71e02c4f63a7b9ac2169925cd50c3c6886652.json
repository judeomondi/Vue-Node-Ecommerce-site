    };\n    },\n    computed: {\n        itemsInCart(){\n          return this.cartItems.some(item => item.id === this.$route.params.productId);\n        }\n    },\n    methods: {\n        async addToCart (){\n          await axios.post('/api/users/12345/cart', {id: this.$route.params.productId});\n          alert ('Successfully added product to cart!');\n        }\n    },\n    components: { NotFoundPage },\n    async created(){\n      const response = await axios.get(`/api/product/${this.$route.params.productId}`);\n      const product = response.data;\n      this.product = product;\n\n      const cartItemResponse = await axios.get('/api/users/12345/cart');\n      const cartItems = response.data;\n      this.cartItems = cartItems;\n    }\n}\n</script>"],"mappings":"AAsBA,OAAOA,YAAW,MAAO,0BAA0B;AACnD,OAAOC,KAAI,MAAO,OAAO;AAIzB,eAAe;EACXC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,CAAC,CAAC;MACXC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,WAAWA,CAAA,EAAE;MACX,OAAO,IAAI,CAACF,SAAS,CAACG,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAM,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,SAAS,CAAC;IAC9E;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,SAAQA,CAAA,EAAI;MAChB,MAAMd,KAAK,CAACe,IAAI,CAAC,uBAAuB,EAAE;QAACN,EAAE,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC;MAAS,CAAC,CAAC;MAC7EI,KAAI,CAAG,qCAAqC,CAAC;IAC/C;EACJ,CAAC;EACDC,UAAU,EAAE;IAAElB;EAAa,CAAC;EAC5B,MAAMmB,OAAOA,CAAA,EAAE;IACb,MAAMC,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAE,gBAAe,IAAI,CAACV,MAAM,CAACC,MAAM,CAACC,SAAU,EAAC,CAAC;IAChF,MAAMT,OAAM,GAAIgB,QAAQ,CAACjB,IAAI;IAC7B,IAAI,CAACC,OAAM,GAAIA,OAAO;IAEtB,MAAMkB,gBAAe,GAAI,MAAMrB,KAAK,CAACoB,GAAG,CAAC,uBAAuB,CAAC;IACjE,MAAMhB,SAAQ,GAAIe,QAAQ,CAACjB,IAAI;IAC/B,IAAI,CAACE,SAAQ,GAAIA,SAAS;EAC5B;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}